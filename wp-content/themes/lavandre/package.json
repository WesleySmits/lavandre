{
    "name": "lavandre",
    "version": "0.0.1",
    "description": "Website for LavandrÃ©",
    "husky": {
        "hooks": {
            "pre-commit": "yarn prod && yarn test && yarn cypress"
        }
    },
    "scripts": {
        "start": "npm-run-all --parallel dev:watch lint:watch",
        "dev": "./node_modules/.bin/encore dev",
        "dev:watch": "./node_modules/.bin/encore dev --watch",
        "prod": "./node_modules/.bin/encore production",
        "prod:watch": "yarn prod --watch",
        "lint": "esw ./config/webpack.config.* app --color",
        "lint:watch": "yarn lint -- --watch",
        "test": "jest --coverage",
        "test:watch": "yarn test -- --watch",
        "cypress": "npx cypress run",
        "cypress:open": "npx cypress open"
    },
    "browserslist": [
        "defaults"
    ],
    "author": "Wesley Smits",
    "license": "MIT",
